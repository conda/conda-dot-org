{
  "name": "conda-dot-org",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "./bin/populate-minutes && ./bin/populate-ceps && docusaurus start",
    "build": "./bin/populate-minutes && ./bin/populate-ceps && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "stylelint '**/*.css' --fix --ignore-pattern build/**/*.css",
    "install-banner": "npm install --silent --prefix ./bin/banner",
    "banner": "npm run install-banner && node -- ./bin/banner/index.js",
    "install-index-blog": "npm run install-banner && npm install --silent --prefix ./bin/index-blog",
    "index-blog": "npm run install-index-blog && node -- ./bin/index-blog/index.js"
  },
  "dependencies": {
    "@docusaurus/core": "^3.3.2",
    "@docusaurus/plugin-ideal-image": "^3.3.2",
    "@docusaurus/preset-classic": "^3.3.2",
    "@docusaurus/theme-mermaid": "^3.3.2",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/react": "^3.0.0",
    "canvas": "^2.11.2",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.3.2",
    "@docusaurus/types": "^3.3.2",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
